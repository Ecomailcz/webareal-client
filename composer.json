{
  "name": "ecomailcz/webareal-client",
  "description": "Basic client for working with Webareal.cz API",
  "authors": [
    {
      "name": "Jan Tlapák",
      "email": "jan@ecomail.cz"
    },
    {
      "name": "jakub Bouček",
      "email": "pan@jakubboucek.cz"
    }
  ],
  "license": ["MIT"],
  "require": {
    "php": "~7.3|~8.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "sllh/composer-versions-check": "^2.0"
